{
  "name": "@falkor/falkor-bundler",
  "version": "1.0.0",
  "description": "ES6 TypeScript bundler to be used with the Falkor Framework",
  "author": "Barnabas Bucsy (https://github.com/theonethread)",
  "license": "MIT",
  "repository": "github:theonethread/falkor-bundler",
  "keywords": [
    "falkor",
    "bundler",
    "ES6",
    "library",
    "lib",
    "binary",
    "bin"
  ],
  "type": "module",
  "main": ".dist/index.js",
  "bin": ".dist/index.js",
  "files": [
    ".dist",
    ".man"
  ],
  "man": ".man/falkor-bundler.1",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "debug": "rimraf .dist/**/* && node src/index.js --debug --input src/index.js",
    "release": "rimraf .dist/**/* && node src/index.js --release --input src/index.js",
    "man": "pandoc --version && (mkdir .man || echo folder exists) && rimraf .man/**/* && pandoc --standalone --to man ./man/man.md --output ./.man/falkor-bundler.1",
    "prepublishOnly": "npm run release && npm run man"
  },
  "devDependencies": {
    "@types/node": "14.17.6",
    "@types/minimist": "1.2.2",
    "@types/shelljs": "0.8.9",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "minimist": "1.2.5",
    "shelljs": "0.8.4",
    "strip-json-comments": "4.0.0",
    "typescript": "4.4.3",
    "tslib": "2.3.1",
    "rollup": "2.57.0",
    "builtin-modules": "3.2.0",
    "rollup-plugin-node-externals": "2.2.0",
    "@rollup/plugin-typescript": "8.2.5",
    "rollup-plugin-jscc": "2.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-dts": "4.0.0"
  }
}
