{
  "name": "@falkor/falkor-bundler",
  "version": "1.1.3",
  "description": "ES6 JavaScript / TypeScript bundler to be used with the Falkor Framework",
  "author": {
    "name": "Barnabas Bucsy",
    "url": "https://github.com/theonethread"
  },
  "contributors": [
    {
      "name": "Barnabas Bucsy",
      "url": "https://github.com/theonethread"
    }
  ],
  "license": "MIT",
  "repository": "github:theonethread/falkor-bundler",
  "homepage": "https://falkor.world/",
  "funding": {
    "type": "individual",
    "url": "https://www.linkedin.com/in/barnabas-bucsy"
  },
  "keywords": [
    "falkor",
    "bundler",
    "ES6",
    "build",
    "library",
    "lib",
    "binary",
    "bin"
  ],
  "type": "module",
  "main": ".dist/index.js",
  "bin": ".dist/index.js",
  "shared": ".dist/cli/version.js",
  "files": [
    ".dist",
    ".man"
  ],
  "man": ".man/falkor-bundler.1",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "scripts": {
    "debug": "rimraf .dist/**/* && node src/index.js --debug --input src/index.js && node src/index.js --debug --input src/cli/version.js --out .dist/cli",
    "release": "rimraf .dist/**/* && node src/index.js --release --input src/index.js && node src/index.js --release --input src/cli/version.js --out .dist/cli",
    "man": "pandoc --version && (mkdir .man || rimraf .man/**/*) && pandoc --standalone --to man ./man/man.md --output ./.man/falkor-bundler.1",
    "prepublishOnly": "npm run release && npm run man"
  },
  "devDependencies": {
    "@types/node": "14.17.21",
    "@types/minimist": "1.2.2",
    "@types/shelljs": "0.8.9",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "minimist": "1.2.5",
    "shelljs": "0.8.4",
    "strip-json-comments": "4.0.0",
    "typescript": "4.4.4",
    "tslib": "2.3.1",
    "rollup": "2.58.0",
    "builtin-modules": "3.2.0",
    "rollup-plugin-node-externals": "2.2.0",
    "@rollup/plugin-typescript": "8.2.5",
    "rollup-plugin-jscc": "2.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-dts": "4.0.0"
  }
}
